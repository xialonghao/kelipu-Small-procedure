/* pages/jiesuan/jiesuan.wxss */
@import "../public/gobal.wxss";
page {
  padding-bottom: 300rpx;
  background: #f4f4f4;
}

.no_mess {
  height: 83rpx;
  line-height: 83rpx;
  padding: 0 30rpx;
  font-size: 24rpx;
  color: #262626;
  box-sizing: border-box;
  background: #fff url(http://giftcardapp.colipu.com/static/module/admin/img/shop_set_img1.png) no-repeat center right 30rpx;
  background-size: 12rpx 21rpx;
  margin: 15rpx 0;
  border: none!important;
  border-radius: 0!important;
  text-align: left;
}
button::after{
border: none;
}
.address_mess {
  background: #fff;
  padding: 30rpx;
  box-sizing: border-box;
  padding-bottom: 23rpx;
  position: relative;
  background: #fff url(http://giftcardapp.colipu.com/static/module/admin/img/shop_set_img1.png) no-repeat center right 30rpx;
  background-size: 12rpx 21rpx;
  padding-top: 25rpx;
}

.shop_set_adres {
  margin: 15rpx 0;
}

.address_mess::after {
  content: '';
  clear: both;
  display: block;
}

.shop_set_bg {
  background: #fff;
  padding: 0 30rpx;
}

.shop_set_pro {
  border-bottom: 1rpx solid #efefef;
  display: flex;
  justify-content: space-between;
  flex-wrap: nowrap;
  height: 200rpx;
  box-sizing: border-box;
  background: #fff;
}

.shop_price {
  position: absolute;
  left: 0;
  bottom: 30rpx;
  font-size: 26rpx;
  color: #d71635;
  font-weight: bold;
  max-width: 350rpx;
  line-height: 36rpx;
}

.shop_price text {
  color: #d70029;
  font-size: 24rpx;
  display: inline-block;
  margin-left: 15rpx;
  font-weight: normal;
  line-height: 36rpx;
}

.shop_set_pro .shop_set_left {
  width: 150rpx;
  height: 150rpx;
  justify-content: center;
  align-items: center;
  display: flex;
  margin-top: 25rpx;
}

.shop_set_pro .shop_set_right {
  margin-left: 25rpx;
  width: 515rpx;
  position: relative;
}

.shop_set_right .shop_titi {
  color: #252525;
  font-size: 26rpx;
  line-height: 46rpx;
  white-space: normal;
  margin-top: 29rpx;
  display: block;
  text-align: left;
  word-break:break-all;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.shop_set_right .shop_set_num {
  font-size: 26rpx;
  line-height: 36rpx;
  color: #999;
  position: absolute;
  right: 0;
  bottom: 30rpx;
}

.shop_dis, .shop_mess {
  background: #fff;
  display: flex;
  justify-content: space-between;
  flex-wrap: nowrap;
  margin: 0 30rpx;
  border-bottom: 1rpx solid #efefef;
}
.shop_mess{
  border-bottom: none;
}
.white_bg {
  background: #fff;
}

.shop_dis view, .shop_mess view {
  font-size: 26rpx;
  color: #252525;
  height: 94rpx;
  line-height: 94rpx;
}

.shop_dis text {
  font-size: 24rpx;
  color: #999;
  display: block;
  height: 94rpx;
  line-height: 94rpx;
}

.shop_mess input {
  width: 530rpx;
  border: none;
  font-size: 24rpx;
  color: #999;
  height: 94rpx;
  line-height: 94rpx;
}

.shop_total_price {
  border-top: 15rpx solid #f4f4f4;
}

.shop_total_price, .shop_freight {
  background: #fff;
  padding: 0 30rpx;
  padding-top: 35rpx;
  display: flex;
  justify-content: space-between;
}

.shop_total_price view, .shop_freight view {
  font-size: 26rpx;
  color: #252525;
}

.shop_total_price text, .shop_freight text {
  font-size: 24rpx;
  color: #d70029;
}

.shop_freight {
  margin-top: 0;
  padding-bottom: 30rpx;
  border-bottom: 1rpx solid #efefef;
}

.shop_pay {
  height: 80rpx;
  line-height: 80rpx;
  text-align: right;
  padding-right: 30rpx;
  font-size: 26rpx;
  color: #252525;
  background: #fff;
}

.shop_pay text {
  font-size: 26rpx;
  color: #d70029;
}

.shop_gpay {
  display: flex;
  justify-content: space-between;
  flex-wrap: nowrap;
  position: fixed;
  width: 750rpx;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  background: #fff;
  z-index: 99;
  border-top: 1rpx solid #efefef;
}

.shop_gpay .shop_integ {
  width: 480rpx;
  height: 120rpx;
  display: block;
  line-height: 120rpx;
  background: #fff;
  font-size: 26rpx;
  color: #252525;
  padding-left: 30rpx;
  box-sizing: border-box;
}

.shop_gpay .shop_integ .color {
  color: #d7012a;
  font-size: 26rpx;
  margin: 0 10rpx;
  white-space: pre-wrap;
  overflow: auto;
  width: sale();
}

.shop_integ cover-view {
  float: left;
  display: block;
  height: 120rpx;
  line-height: 120rpx;
}

.shop_gpay .shop_gpayon {
  width: 270rpx;
  height: 120rpx;
  background: #d7012a;
  color: #fff;
  font-size: 30rpx;
  border-radius: 0;
  line-height: 120rpx;
  text-align: center;
}

.adres_left, .adres_right {
  float: left;
}

.adres_left {
  width: 120rpx;
  word-wrap: break-word;
  box-sizing: border-box;
  padding-right: 10rpx;
}

.adres_right {
  width: 505rpx;
}

.adres_left view {
  font-size: 26rpx;
  color: #252525;
  margin-bottom: 13rpx;
  line-height: 36rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.adres_left text {
  width: 66rpx;
  height: 33rpx;
  border: 1rpx solid #df3354;
  border-radius: 5rpx;
  font-size: 20rpx;
  color: #d70029;
  display: block;
  text-align: center;
  line-height: 35rpx;
}

.adres_right view {
  font-size: 26rpx;
  line-height: 36rpx;
  color: #252525;
}

.adres_right text {
  font-size: 24rpx;
  color: #666;
  display: block;
  margin-top: 22rpx;
  line-height: 38rpx;
  text-align: justify;
  overflow: hidden;
  white-space:nowrap;
  text-overflow:ellipsis;
}
.modal-box {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0px;
  background: rgba(0, 0, 0, 0.5);
  overflow: hidden;
  display: flex;
  align-content: center;
  justify-content: center;
  align-items: center;
  z-index: 8;
}
.modal-body-two .content .one {
  background: url(http://giftcardapp.colipu.com/static/module/admin/img/img14.png) no-repeat 36rpx center;
  background-size: 40rpx 30rpx;
}
.modal-body {
  background: #fff;
  width: 690rpx;
  box-sizing: border-box;
  padding: 40rpx 64rpx 46rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  z-index: 9;
  transform: translate(-50%, -50%);
}
.modal-body .title {
  font-size: 30rpx;
  color: #252525;
  line-height: 30rpx;
  padding-bottom: 36rpx;
  border-bottom: 1rpx solid #eee;
  text-align: center;
  position: relative;
}
.modal-body .content .binding {
  width: 50%;
  float:left;
  height: 101rpx;
  line-height: 101rpx;
  border: 0;
  background: #d70029;
  color: #fff;
  border-radius: 0;
  font-size: 30rpx;
  margin-top: 24rpx;
}
.modal-body .content .bindings {
  width: 50%;
  left:15px;
  top:11px;
  height: 101rpx;
  line-height: 101rpx;
  border: 0;
  background: #d70029;
  color: #fff;
  border-radius: 0;
  font-size: 30rpx;
  margin-top: 24rpx;
}
.pay{
  position: fixed;
  text-align: center;
  width: 640rpx;
  height: 500rpx;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  z-index: 999;
  background: #fff;
}
.modal-box{
  background: rgba(0, 0, 0, 0.4);
  position: fixed;
  width: 100%;
  height: 100%;
  z-index: 998;
  top: 0;
  display: block;
}
.pay .pay_tit{
  font-size: 32rpx;
  color: #252525;
  display: block;
  height: 95rpx;
  line-height: 95rpx;
  border-bottom: 1rpx solid rgba(239, 162, 174, 0.99);
  margin-bottom: 70rpx;
}
.pay .fl{
  margin-left: 125rpx;
}
.pay .fl image{
  width: 100rpx;
  height: 100rpx; 
}
.pay .fl,
.pay .fr{
  float: left;
  font-size: 0;
}
.clear::after{
  content: '';
  display: block;
  clear: both;
}
.pay .fr{
  text-align: left;
  height: 100rpx;
  margin-left: 50rpx;
}
.pay .fr view{
  font-size: 30rpx;
  color: #252525;
  line-height: 1rem;
}
.pay .fr text{
font-size: 48rpx;
color: #d70029;
display: block;
margin-top: 10rpx;
line-height: 60rpx;
width: 310rpx;
font-weight: bold;
}
.pay .pay_click{
  border-top: 1rpx solid rgba(239, 239, 239, 0.99);
  width: 548rpx;
  margin: 0 auto;
  margin-top: 70rpx;
  padding-top: 38rpx;
}
.pay .pay_click button{
  width: 548rpx;
  height: 84rpx;
  line-height: 84rpx;
  background: #d70029;
  color: #fff;
}
.pay .close_box{
  width: 50rpx;
  height: 50rpx;
  background: url(http://giftcardapp.colipu.com/static/module/admin/img/close_box.jpg) no-repeat center;
  background-size: 26rpx 25rpx;
  position: absolute;
  top: 24rpx;
  left: 37rpx;
}
.pay_mess{
  width: 690rpx;
  background: #fff;
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  z-index: 99999;
  height: 445rpx;
}
.pay_mess image{
  width: 93rpx;
  height: 93rpx;
}
.pay_mess .pay_mess_tit{
  font-size: 30rpx;
  color: #252525;
  display: block;
  text-align: center;
  padding: 40rpx 0;
  background: url(http://giftcardapp.colipu.com/static/module/admin/img/pay_borimg1.png) no-repeat center bottom;
  background-size: 566rpx 8rpx;
  position: relative;
  margin: 0 65rpx;
}
.pay_mess .pay_mess_img{
  text-align: center;
  padding: 55rpx 0;
  padding-left: 128rpx;
  display: block;
  overflow: hidden;
}
.pay_mess .pay_mess_img::after{
  content: '';
  display: block;
  clear: both;
}
.pay_mess .pay_mess_img .fl,
.pay_mess .pay_mess_img .fr{
  float: left;
  height: 93rpx;
}
.pay_mess .pay_mess_img .fr{
  font-size: 30rpx;
  line-height: 93rpx;
  color: #252525;
  margin-left: 32rpx;
}
.pay_mess .pay_mess_img .fr view,
.pay_mess .pay_mess_img .fr text{
  line-height: 30rpx;
  float: left;
  padding: 30rpx 0;
}
.pay_mess .pay_mess_img .fr text{
  color: #d70029;
  max-width: 210rpx;
  display: inline-block;
  word-break:break-all;
  margin-left: 10rpx; 
}
.pay_mess .pay_mess_btn{
  text-align: center;
}
.pay_mess .pay_mess_btn view,
.pay_mess .pay_mess_btn navigator{
  width: 263rpx;
  height: 64rpx;
  display: inline-block;
  text-align: center;
  border: 1px solid #d70029;
  font-size: 30rpx;
  color: #d70029;
  line-height: 66rpx;
  margin: 0 18rpx;
}
.shop_set_left{
  position: relative;
}
.shop_set_left .hide{
  width: 100%;
  height: 40rpx;
  background: rgba(0,0,0,0.5);
  position: absolute;
  left: 0;
  bottom: 0;
  font-size: 24rpx;
  line-height: 40rpx;
  color: #fff;
  text-align: center;
}







