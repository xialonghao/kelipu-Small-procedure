.modal-body1 {
  background: rgba(0, 0, 0, 0.6);
  box-sizing: border-box;
  padding: 40rpx 30rpx;
  position: fixed;
  top: 50%;
  font-size: 26rpx;
  color: #fff;
  left: 50%;
  z-index: 9;
  transform: translate(-50%, -50%);
  border-radius: 8rpx;
  text-align: center;
}
.modal-body1 view{
  margin-top: 20rpx;
}